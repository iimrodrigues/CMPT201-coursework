#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <sys/types.h>
#include <sys/wait.h>
#include <unistd.h>
#include <stdlib.h>

int main() {
  pid_t pid = fork();

  char *input = NULL;
  size_t length = 0;
  getline(&input, &length, stdin);


  if (pid == 0) {
    //child
  } else {
    if (waitpid(pid, NULL, 0) == -1){
        perror("waitpid");
        exit(EXIT_FAILURE);
      }
  }
}
